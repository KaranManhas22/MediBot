<configuration>
  <system.webServer>
    <handlers>
      <add name="python"
           path="main.py"
           verb="*"
           modules="FastCgiModule"
           scriptProcessor="D:\home\python364x64\python.exe|D:\home\python364x64\wfastcgi.py"
           resourceType="Unspecified"
           requireAccess="Script" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="Static Files" stopProcessing="true">
          <conditions>
            <add input="true" pattern="false" />
          </conditions>
          <action type="None" />
        </rule>
        <rule name="Dynamic Handler" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="main.py" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>